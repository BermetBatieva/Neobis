import java.util.Scanner;

public class foobar {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        if (a % 2 == 0) {
            System.out.println("foo <the integer that was given>");
        } else {
            System.out.println("bar <the integer that was given>");
        }
        System.exit(0);

    }
}
